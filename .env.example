# Example environment variables for back-end
# Copy to .env and adjust values as needed

# HTTP server port
PORT=3000

# Database (PostgreSQL)
POSTGRES_USER=picnic
POSTGRES_PASSWORD=picnic
POSTGRES_DB=picnic
POSTGRES_PORT=5432

# Prisma connection URL
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@localhost:${POSTGRES_PORT}/${POSTGRES_DB}?schema=public
